{
  "manifest_version": 3,
  "name": "Kindle Auto Timer",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
